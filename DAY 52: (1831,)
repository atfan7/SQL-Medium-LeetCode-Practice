PROBLEM 1831:::

with cte as (
select 
transaction_id ,amount, substr(day,1,10) as datte
from Transactions
),
cte1 as(
select transaction_id,datte,amount, rank() over(partition by datte order by amount desc) as rnk


from cte 


)

select transaction_id
from cte1 
where rnk=1
order by transaction_id asc
